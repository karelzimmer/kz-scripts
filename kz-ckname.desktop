[Desktop Entry]
Type=Application
Name=Check file names
Name[nl]=Bestandsnamen controleren
GenericName=Check program
GenericName[nl]=Controleprogramma
Comment=Checks NTFS folder and file names
Comment[nl]=Controleert NTFS map- en bestandsnamen
Keywords=kzckname;ckname;check;name;
Keywords[nl]=kzckname;;ckname;check;naam;
Categories=KZ Scripts;
Exec=/usr/bin/kz-ckname --gui
Icon=/usr/share/pixmaps/kz-ckname.svg
Terminal=true
Actions=version;

[Desktop Action version]
Name=Show version information
Name[nl]=Versie-informatie tonen
Exec=/usr/bin/kz-ckname --gui --version

