# shellcheck shell=bash
###############################################################################
# Tab-completion-script for kz-install.
#
# SPDX-FileCopyrightText: Karel Zimmer <info@karelzimmer.nl>
# SPDX-License-Identifier: CC0-1.0
###############################################################################

dashoptions='-a --apps -c --cat  -f --file -g --gui -l --list -h --help -r
    --remove -u --usage -m --manual -v --version'
complete -o default -W "$dashoptions" kz-install
