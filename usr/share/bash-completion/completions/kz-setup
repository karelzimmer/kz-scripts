# shellcheck shell=bash
###############################################################################
# Tab-completion-script for kz-setup.
#
# Written by Karel Zimmer <info@karelzimmer.nl>
# CC0 1.0 <https://creativecommons.org/publicdomain/zero/1.0>.
###############################################################################

dashoptions='-a --apps -c --cat -f --file -g --gui -l --list -h --help -r
    --reset -u --usage -v --version'
complete -o default -W "$dashoptions" kz-setup
