# shellcheck shell=bash
###############################################################################
# Tab-completion-script for kz-restore.
#
# Written by Karel Zimmer <info@karelzimmer.nl> CC0 1.0
# <https://creativecommons.org/publicdomain/zero/1.0>.
###############################################################################

dashoptions='-d --dry-run  -g --gui -s --source -h --help -u --usage -v
    --version'
complete -o default -W "$dashoptions" kz-restore
